{
	"manifest_version": 2,
	"name": "Prototype",
	"version": "0.4.0",
	"description": "A scratch space for notes, links, reminders, and whatever else you want to keep around.",
	"author": "Joel Gustafson",
	"developer": {
		"name": "nDimensional Studios",
		"url": "https://en.wikipedia.org/wiki/Walt_Whitman"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"homepage_url": "https://github.com/nDimensional/prototype",
	"icons": {
		"48": "icon-48.png",
		"96": "icon-96.png",
		"192": "icon-192.png"
	},
	"permissions": ["storage"],
	"applications": {
		"gecko": {
			"id": "{4c6d75f6-cdad-4947-aec4-90a071b7fb81}",
			"strict_min_version": "55.0"
		}
	},
	"commands": {
		"toggle-settings": {
			"suggested_key": {
				"default": "Ctrl+Period"
			},
			"description": "Toggle the settings panel"
		}
	}
}
